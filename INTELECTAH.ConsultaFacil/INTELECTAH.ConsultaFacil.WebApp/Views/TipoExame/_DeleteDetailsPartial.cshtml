@model INTELECTAH.ConsultaFacil.ViewModel.Implementations.TipoExameViewModel

<div class="panel-body">
    <div class="form-horizontal">
        <div class="form-group">
            <label asp-for="TipoExameId" class="control-label col-md-2"></label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.TipoExameId, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Nome" class="control-label col-md-2"></label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.Nome, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Descricao" class="control-label col-md-2"></label>
            <div class="col-md-10">
                @Html.TextAreaFor(model => model.Descricao, new { @class = "form-control", disabled = "disabled" })
            </div>
        </div>
    </div>
</div>