@model INTELECTAH.ConsultaFacil.ViewModel.Implementations.ConsultaViewModel

<div class="panel-body">
    <div class="form-horizontal">
        <div class="form-group">
            <label asp-for="ConsultaId" class="control-label col-md-2"></label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.ConsultaId, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Paciente" class="control-label col-md-2"></label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.Paciente.Nome, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Exame" class="control-label col-md-2"></label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.Exame.Nome, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            <label asp-for="DataHora" class="control-label col-md-2"></label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.DataHora, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
        <div class="form-group">
            <label asp-for="Protocolo" class="control-label col-md-2"></label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.Protocolo, new { htmlAttributes = new { @class = "form-control", disabled = "disabled" } })
            </div>
        </div>
    </div>
</div>